info\">职员</el-tag>\r\n          <el-tag v-if=\"item.name == '人事经理'\" type=\"success\">人事经理</el-tag>\r\n        </span>\r\n        <el-card :body-style=\"{ padding: '0px' }\">\r\n          <img :src=\"baseApi + clerk.photo\" class=\"image\" />\r\n          <div style=\"padding: 14px\">\r\n            <span>姓名:{{ clerk.name }}</span>\r\n            <div class=\"bottom clearfix\">\r\n              <div>编号:{{ clerk.id }}</div>\r\n              <div>性别:{{ (clerk.sex = 0 ? \"女\" : \"男\") }}</div>\r\n              <div>手机号:{{ clerk.phoneNumber }}</div>\r\n              <div>身份证号:{{ clerk.idCard }}</div>\r\n              <div>家庭住址:{{ clerk.address }}</div>\r\n              <div>部门:{{ clerk.departmentId }}</div>\r\n              <div>职位:{{ clerk.position }}</div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <!-- 权限 -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport clerk from \"@/api/clerk\";\r\nimport ImageCropper from \"@/components/ImageCropper\";\r\nimport PanThumb from \"@/components/PanThumb\";\r\n\r\nexport default {\r\n  components: { ImageCropper, PanThumb },\r\n  data() {\r\n    return {\r\n      baseApi:  process.env.VUE_APP_BASE_API,\r\n      roleList: [],\r\n      clerk: {},\r\n      saveBtnDisabled: false, // 保存按钮是否禁用,\r\n      saveBtnDisabled: false, // 保存按钮是否禁用,\r\n      imagecropperShow: false, //上传弹窗组件是否显示\r\n      imagecropperKey: 0, //上传组件的key值\r\n    };\r\n  },\r\n  created() {\r\n    clerk.getClerkAndUserRole().then((response) => {\r\n      console.log(response);\r\n      this.roleList = response.data.roleList;\r\n      this.clerk = response.data.clerk;\r\n    });\r\n  },\r\n  methods: {\r\n    close() {\r\n      //关闭上传弹框的方法\r\n      this.imagecropperShow = false;\r\n      //上传组件初始化\r\n      this.imagecropperKey = this.imagecropperKey + 1;\r\n    },\r\n    //上传成功方法\r\n    cropSuccess(data) {\r\n      this.imagecropperShow = false;\r\n      //上传之后接口返回图片地址\r\n      this.clerk.photo = data.url;\r\n      this.imagecropperKey = this.imagecropperKey + 1;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.time {\r\n  font-size: 13px;\r\n  color: #999;\r\n}\r\n\r\n.bottom {\r\n  margin-top: 13px;\r\n  line-height: 30px;\r\n}\r\n\r\n.button {\r\n  padding: 0;\r\n  float: right;\r\n}\r\n\r\n.image {\r\n  width: 100%;\r\n  display: block;\r\n}\r\n\r\n.clearfix:before,\r\n.clearfix:after {\r\n  display: table;\r\n  content: \"\";\r\n}\r\n\r\n.clearfix:after {\r\n  clear: both;\r\n}\r\n</style>\r\n\r\n"]}]}