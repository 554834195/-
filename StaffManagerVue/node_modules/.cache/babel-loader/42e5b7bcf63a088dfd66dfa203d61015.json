">\r\n              <span>{{ props.row.idCard }}</span> </el-form-item\r\n            ><br />\r\n            <el-form-item label=\"员工住址\">\r\n              <span>{{ props.row.address }}</span>\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"序号\" width=\"70\" align=\"center\">\r\n        <template slot-scope=\"scope\">{{\r\n          (page - 1) * limit + scope.$index + 1\r\n        }}</template>\r\n      </el-table-column>\r\n\r\n      <el-table-column prop=\"id\" sortable label=\"员工编号\" width=\"200\" />\r\n      <el-table-column prop=\"name\" sortable label=\"员工姓名\" width=\"100\" />\r\n      <el-table-column prop=\"position\" sortable label=\"员工职位\" />\r\n      <el-table-column\r\n        prop=\"departmentId\"\r\n        sortable\r\n        label=\"员工部门\"\r\n        width=\"100\"\r\n      />\r\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <router-link :to=\"'/permissions/updateClerk/' + scope.row.id\">\r\n            <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-edit\"\r\n              >权限修改</el-button\r\n            >\r\n          </router-link>\r\n          <router-link :to=\"'/clerkThrough/showClerkThrough/' + scope.row.id\">\r\n            <el-button type=\"info\" size=\"mini\"\r\n              >员工经历</el-button\r\n            >\r\n          </router-link>\r\n          \r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      :current-page=\"page\"\r\n      :page-size=\"limit\"\r\n      :total=\"total\"\r\n      style=\"padding: 30px 0; text-align: center\"\r\n      layout=\"total, prev, pager, next, jumper\"\r\n      @current-change=\"getClerkListPage\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport clerk from \"@/api/clerk\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      list: null, //查询之后接口返回集合\r\n      page: 1, //当前页\r\n      limit: 6, //每页记录数\r\n      total: 0, //总记录数\r\n      clerkQueryVo: {\r\n        name: \"\",\r\n        position: \"\",\r\n        departmentName: \"\",\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    //页面渲染之前执行，一般用于调用methods中定义的方法\r\n    this.getClerkListPage();\r\n  },\r\n  methods: {\r\n    resetData() {\r\n      //清空的方法\r\n      //表单数据清空\r\n      this.clerkQueryVo = {\r\n        name: \"\",\r\n        position: \"\",\r\n        departmentName: \"\",\r\n      };\r\n      //查询所有\r\n      this.getClerkListPage();\r\n    },\r\n    //页面渲染之后执行，一般用于创建具体方法\r\n    //列表方法\r\n    getClerkListPage(page = 1) {\r\n      this.page = page;\r\n      clerk\r\n        .getClerkListPage(this.page, this.limit, this.clerkQueryVo)\r\n        .then((response) => {\r\n          //请求成功\r\n          //response接口返回的数据\r\n          this.list = response.data.clerkList;\r\n          this.total = response.data.total;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    deleteClerkById(id) {\r\n      //删除功能\r\n      this.$confirm(\"此操作将永久删除该记录, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      }).then(() => {\r\n        clerk\r\n          .deleteClerkById(id)\r\n          .then((response) => {\r\n            //删除成功\r\n            //提示信息\r\n            this.$message({\r\n              type: \"success\",\r\n              message: \"删除成功!\",\r\n            });\r\n            //回到列表\r\n            this.getClerkListPage();\r\n          })\r\n          .catch((error) => {\r\n            //提示信息\r\n            this.$message({\r\n              type: \"warning\",\r\n              message: \"删除失败!\",\r\n            });\r\n          });\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2GA,OAAAA