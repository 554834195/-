"length","undefined","response","attendanceList","catch","error","console","log","_defineProperty","deleteAttendanceById","id","_this3","$confirm","confirmButtonText","cancelButtonText","type","$message","message"],"sources":["src/views/attendance/listAttendanceThrough.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <!--查询表单-->\r\n    <el-form :inline=\"true\" class=\"demo-form-inline\">\r\n      <el-form-item>\r\n        <el-input\r\n          v-model=\"attendanceQueryVo.clerkName\"\r\n          placeholder=\"员工名称\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-input\r\n          v-model=\"attendanceQueryVo.departmentName\"\r\n          placeholder=\"部门名称\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-select\r\n          clearable\r\n          v-model=\"attendanceQueryVo.status\"\r\n          placeholder=\"请选择考勤类型\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in options\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\"\r\n          >\r\n          </el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n\r\n      <el-button\r\n        type=\"primary\"\r\n        icon=\"el-icon-search\"\r\n        @click=\"getAttendanceListPage()\"\r\n        >查询</el-button\r\n      >\r\n      <el-button type=\"default\" @click=\"resetData()\">清空</el-button>\r\n      <el-button\r\n        :loading=\"downloadLoading\"\r\n        style=\"margin: 0 0 20px 20px\"\r\n        type=\"primary\"\r\n        icon=\"el-icon-document\"\r\n        @click=\"handleDownload\"\r\n      >\r\n        下载Excel\r\n      </el-button>\r\n    </el-form>\r\n    <el-table\r\n      :data=\"list\"\r\n      element-loading-text=\"数据加载中\"\r\n      fit\r\n      highlight-current-row\r\n    >\r\n      <el-table-column label=\"序号\" width=\"70\" align=\"center\">\r\n        <template slot-scope=\"scope\">{{\r\n          (page - 1) * limit + scope.$index + 1\r\n        }}</template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"id\" sortable label=\"编号\" width=\"200\" />\r\n      <el-table-column prop=\"time\" sortable label=\"日期\" width=\"200\" />\r\n      <el-table-column prop=\"status\" label=\"类型\" />\r\n      <el-table-column prop=\"clerkId\" sortable label=\"员工姓名\" />\r\n      <el-table-column prop=\"departmentId\" sortable label=\"员工部门\" />\r\n      <el-table-column\r\n        v-if=\"isAdmin == true\"\r\n        label=\"操作\"\r\n        width=\"200\"\r\n        align=\"center\"\r\n      >\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            type=\"danger\"\r\n            size=\"mini\"\r\n            icon=\"el-icon-delete\"\r\n            @click=\"deleteAttendanceById(scope.row.id)\"\r\n            >删除</el-button\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      :current-page=\"page\"\r\n      :page-size=\"limit\"\r\n      :total=\"total\"\r\n      style=\"padding: 30px 0; text-align: center\"\r\n      layout=\"total, prev, pager, next, jumper\"\r\n      @current-change=\"getAttendanceListPage\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport attendance from \"@/api/attendance\";\r\nimport { mapGetters } from \"vuex\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      downloadLoading: false,\r\n      //            事假1,迟到2,早退3,病假4,旷工5,休补6\r\n      options: [\r\n        {\r\n          value: \"1\",\r\n          label: \"事假\",\r\n        },\r\n        {\r\n          value: \"2\",\r\n          label: \"迟到\",\r\n        },\r\n        {\r\n          value: \"3\",\r\n          label: \"早退\",\r\n        },\r\n        {\r\n          value: \"4\",\r\n          label: \"病假\",\r\n        },\r\n        {\r\n          value: \"5\",\r\n          label: \"旷工\",\r\n        },\r\n        {\r\n          value: \"6\",\r\n          label: \"休补\",\r\n        },\r\n      ],\r\n      list: null, //查询之后接口返回集合\r\n      page: 1, //当前页\r\n      limit: 6, //每页记录数\r\n      total: 0, //总记录数\r\n      attendanceQueryVo: {\r\n        audit: \"1\",\r\n      },\r\n      isAdmin: false,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"roles\"]),\r\n  },\r\n  created() {\r\n    if (this.roles.includes(\"管理员\")) {\r\n      this.isAdmin = true;\r\n    }\r\n    this.getAttendanceListPage();\r\n  },\r\n  methods: {\r\n    handleDownload() {\r\n      this.downloadLoading = true;\r\n      import(\"@/vendor/Export2Excel\").then((excel) => {\r\n        const tHeader = [\"编号\", \"员工姓名\", \"部门\", \"时间\", \"类型\"];\r\n        const filterVal = [\"id\", \"clerkId\", \"departmentId\", \"time\", \"status\"];\r\n        const list = this.list;\r\n        const data = this.formatJson(filterVal, list);\r\n        excel.export_json_to_excel({\r\n          header: tHeader,\r\n          data,\r\n          filename: \"考勤信息\",\r\n          autoWidth: true,\r\n          bookType: \"xlsx\",\r\n        });\r\n        this.downloadLoading = false;\r\n      });\r\n    },\r\n    formatJson(filterVal, jsonData) {\r\n      return jsonData.map((v) =>\r\n        filterVal.map((j) => {\r\n          if (j === \"timestamp\") {\r\n            return parseTime(v[j]);\r\n          } else {\r\n            return v[j];\r\n          }\r\n        })\r\n      );\r\n    },\r\n    resetData() {\r\n      //清空的方法\r\n      //表单数据清空\r\n      this.attendanceQueryVo = {};\r\n      //查询所有\r\n      this.getAttendanceListPage();\r\n    },\r\n    //页面渲染之后执行，一般用于创建具体方法\r\n    //列表方法\r\n    getAttendanceListPage(page = 1) {\r\n      this.page = page;\r\n      attendance\r\n        .getAttendanceListPage(this.page, this.limit, this.attendanceQueryVo)\r\n        .then((response) => {\r\n          //请求成功\r\n          //response接口返回的数据\r\n          this.list = response.data.attendanceList;\r\n          this.total = response.data.total;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    resetData() {\r\n      this.getAttendanceListPage();\r\n    },\r\n    deleteAttendanceById(id) {\r\n      //删除功能\r\n      this.$confirm(\"此操作将永久删除该记录, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      }).then(() => {\r\n        attendance\r\n          .deleteAttendanceById(id)\r\n          .then((response) => {\r\n            //删除成功\r\n            //提示信息\r\n            this.$message({\r\n              type: \"success\",\r\n              message: \"删除成功!\",\r\n            });\r\n            //回到列表\r\n            this.getAttendanceListPage();\r\n          })\r\n          .catch((error) => {\r\n            //提示信息\r\n            this.$message({\r\n              type: \"warning\",\r\n              message: \"删除失败!\",\r\n            });\r\n          });\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+FA,OAAAA,UAAA;AACA,SAAAC,UAAA;AACA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,eAAA;MACA;MACAC,OAAA,GACA;QACAC,KAAA;QACAC,KAAA;MACA,GACA;QACAD,KAAA;QACAC,KAAA;MACA,GACA;QACAD,KAAA;QACAC,KAAA;MACA,GACA;QACAD,KAAA;QACAC,KAAA;MACA,GACA;QACAD,KAAA;QACAC,KAAA;MACA,GACA;QACAD,KAAA;QACAC,KAAA;MACA,EACA;MACAC,IAAA;MAAA;MACAC,IAAA;MAAA;MACAC,KAAA;MAAA;MACAC,KAAA;MAAA;MACAC,iBAAA;QACAC,KAAA;MACA;MACAC,OAAA;IACA;EACA;EACAC,QAAA,EAAAC,aAAA,KACAd,UAAA,YACA;EACAe,OAAA,WAAAA,QAAA;IACA,SAAAC,KAAA,CAAAC,QAAA;MACA,KAAAL,OAAA;IACA;IACA,KAAAM,qBAAA;EACA;EACAC,OAAA,GAAAC,QAAA;IACAC,cAAA,WAAAA,eAAA;MAAA,IAAAC,KAAA;MACA,KAAApB,eAAA;MACAqB,OAAA,CAAAC,OAAA,GAAAC,IAAA;QAAA,OAAAC,uBAAA,CAAAC,OAAA;MAAA,GAAAF,IAAA,WAAAG,KAAA;QACA,IAAAC,OAAA;QACA,IAAAC,SAAA;QACA,IAAAxB,IAAA,GAAAgB,KAAA,CAAAhB,IAAA;QACA,IAAAL,IAAA,GAAAqB,KAAA,CAAAS,UAAA,CAAAD,SAAA,EAAAxB,IAAA;QACAsB,KAAA,CAAAI,oBAAA;UACAC,MAAA,EAAAJ,OAAA;UACA5B,IAAA,EAAAA,IAAA;UACAiC,QAAA;UACAC,SAAA;UACAC